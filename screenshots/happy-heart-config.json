  "usage": {
    "quickStart": [
      "1. Install the Happy Heart Theme Collection",
      "2. Open VS Code",
      "3. Press Ctrl+Shift+P (Cmd+Shift+P on Mac)",
      "4. Type 'Preferences: Color Theme'",
      "5. Select any Happy Heart theme",
      "6. Enjoy beautiful syntax highlighting!"
    ],
    "keyboardShortcuts": [
      "Ctrl+Shift+T - Quick theme picker",
      "Ctrl+Shift+Right - Next theme",
      "Ctrl+Shift+Left - Previous theme",
      "Ctrl+Shift+R - Random theme"
    ],
    "customization": [
