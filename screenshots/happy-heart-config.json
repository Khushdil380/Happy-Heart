  "syntaxHighlighting": {
    "tokenTypes": [
      "comment",
      "keyword",
      "string",
      "variable",
      "variable.parameter",
      "constant",
      "number",
      "boolean",
      "function",
      "function.call",
      "class",
      "interface",
      "type",
      "operator",
      "punctuation",
      "tag",
      "attribute",
      "property",
      "namespace",
      "decorator",
      "storage",
      "storage.type",
      "storage.modifier",
      "entity.name.function",
      "entity.name.class",
      "entity.name.type",
      "entity.name.tag",
      "entity.name.namespace",
      "support.function",
      "support.class"
    ],
    "languages": {
      "javascript": {
        "keywords": ["function", "var", "let", "const", "if", "else", "for", "while", "return", "class", "import", "export"],
        "operators": ["+", "-", "*", "/", "%", "=", "==", "===", "!=", "!==", "<", ">", "<=", ">=", "&&", "||", "!"],
        "strings": ["\"", "'", "`"],
        "comments": ["//", "/*", "*/"]
      },
      "typescript": {
        "keywords": ["interface", "type", "enum", "namespace", "module", "declare", "abstract", "public", "private", "protected"],
        "operators": ["+", "-", "*", "/", "%", "=", "==", "===", "!=", "!==", "<", ">", "<=", ">=", "&&", "||", "!"],
        "strings": ["\"", "'", "`"],
        "comments": ["//", "/*", "*/"]
      },
      "python": {
        "keywords": ["def", "class", "if", "else", "elif", "for", "while", "try", "except", "finally", "import", "from", "as"],
        "operators": ["+", "-", "*", "/", "%", "=", "==", "!=", "<", ">", "<=", ">=", "and", "or", "not", "in", "is"],
        "strings": ["\"", "'", "'''", "\"\"\""],
        "comments": ["#"]
      },
      "java": {
        "keywords": ["public", "private", "protected", "class", "interface", "extends", "implements", "static", "final", "abstract"],
        "operators": ["+", "-", "*", "/", "%", "=", "==", "!=", "<", ">", "<=", ">=", "&&", "||", "!"],
        "strings": ["\""],
        "comments": ["//", "/*", "*/"]
      },
      "c": {
        "keywords": ["int", "char", "float", "double", "void", "if", "else", "for", "while", "do", "switch", "case", "break", "continue", "return"],
        "operators": ["+", "-", "*", "/", "%", "=", "==", "!=", "<", ">", "<=", ">=", "&&", "||", "!"],
        "strings": ["\""],
        "comments": ["//", "/*", "*/"]
      }
    }
  },
  "uiTheming": {
    "sections": {
      "titleBar": {
        "properties": ["activeBackground", "activeForeground", "inactiveBackground", "inactiveForeground", "border"],
        "description": "Window title bar styling"
      },
      "menuBar": {
        "properties": ["selectionBackground", "selectionForeground", "background", "foreground"],
        "description": "Menu bar styling"
      },
      "activityBar": {
        "properties": ["background", "foreground", "inactiveForeground", "activeBorder", "badgeBackground", "badgeForeground"],
        "description": "Left activity bar styling"
      },
      "sideBar": {
        "properties": ["background", "foreground", "border", "titleForeground", "sectionHeaderBackground", "sectionHeaderForeground"],
        "description": "Side bar styling"
      },
      "editor": {
        "properties": ["background", "selectionBackground", "selectionHighlightBackground", "wordHighlightBackground", "lineHighlightBackground", "cursorForeground"],
        "description": "Editor area styling"
      },
      "panel": {
        "properties": ["background", "border", "titleActiveForeground", "titleActiveBorder", "titleInactiveForeground"],
        "description": "Bottom panel styling"
      },
      "statusBar": {
        "properties": ["background", "foreground", "noFolderBackground", "debuggingBackground", "itemHoverBackground"],
        "description": "Status bar styling"
      },
      "terminal": {
        "properties": ["background", "foreground", "ansiBlack", "ansiBlue", "ansiCyan", "ansiGreen", "ansiMagenta", "ansiRed", "ansiWhite", "ansiYellow"],
        "description": "Terminal styling"
      },
      "notifications": {
        "properties": ["centerBorder", "centerHeaderBackground", "toastBackground", "background", "foreground", "linkForeground"],
        "description": "Notification styling"
      },
      "quickPick": {
        "properties": ["groupForeground", "groupBorder", "background", "foreground", "focusBackground", "focusForeground"],
        "description": "Quick pick styling"
      }
    }
  },
